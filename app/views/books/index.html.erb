<h1>My Booklist</h1>
 <%= link_to 'Add a new book to your list!', new_book_path %>
 <br>
 <br>
 <br>
<table id="table">
<thead>
  <tr>
    <th id="title">Title</th>
    <th id="author">Author</th>
    <th>Options</th>
  </tr>
</thead>
<tbody>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td>* <%= link_to 'Book Info', book_path(book)%> * <%= link_to 'Edit Book ', edit_book_path(book) %>* <%= link_to 'Delete Book ', book_path(book), method: :delete, data: { confirm: 'Are you sure you want to delete this book?'} %></td>
    </tr>
  <% end %>
</tbody>
</table>
<br>
<br>
<%= link_to 'Home', root_path %>
